{
  "name": "looper.js",
  "version": "2.0.0",
  "description": "Helps find memory leaks",
  "main": "looper.js",
  "dependencies": {},
  "devDependencies": {
    "babel-preset-env": "^1.5.1",
    "jest": "^21.0.2",
    "prettier": "^1.5.3",
    "rollup": "^0.51.8",
    "serve": "^6.0.6"
  },
  "scripts": {
    "test": "jest",
    "prettier": "prettier --single-quote --write \"{src,__{tests,mocks}__}/**/*.js\" \"./*.js\"",
    "serve": "serve --clipless --cache=0 --treeless",
    "build": "rollup looper.js --output.format iife --output.file dist/looper.js --name looper && $(yarn bin)/prettier --single-quote --write \"dist/looper.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/deleteme/looper.js.git"
  },
